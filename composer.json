{
    "name": "methorz/openapi-generator",
    "description": "Automatic OpenAPI 3.0 specification generator from routes and DTOs",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "api",
        "documentation",
        "dto",
        "generator",
        "openapi",
        "openapi3",
        "psr-15",
        "swagger"
    ],
    "authors": [
        {
            "name": "Thorsten Merz",
            "email": "methorz@spammerz.de"
        }
    ],
    "require": {
        "php": "~8.2.0 || ~8.3.0 || ~8.4.0 || ~8.5.0",
        "psr/container": "^1.1 || ^2.0",
        "symfony/console": "^6.0 || ^7.0",
        "symfony/yaml": "^6.0 || ^7.0"
    },
    "require-dev": {
        "nyholm/psr7": "^1.8",
        "phpstan/phpstan": "^2.0",
        "phpstan/phpstan-strict-rules": "^2.0",
        "phpunit/phpunit": "^11.0 || ^12.0",
        "psr/http-message": "^1.1 || ^2.0",
        "psr/http-server-middleware": "^1.0",
        "slevomat/coding-standard": "^8.25",
        "squizlabs/php_codesniffer": "^4.0",
        "symfony/validator": "^6.0 || ^7.0"
    },
    "autoload": {
        "psr-4": {
            "MethorZ\\OpenApi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MethorZ\\OpenApi\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        },
        "sort-packages": true
    },
    "scripts": {
        "analyze": "phpstan analyze --memory-limit=256M",
        "cs-check": "phpcs --standard=phpcs.xml.dist",
        "cs-fix": "phpcbf --standard=phpcs.xml.dist",
        "quality": [
            "@cs-check",
            "@analyze"
        ],
        "test": "phpunit",
        "test:coverage": "phpunit --coverage-html .phpunit.cache/coverage-html"
    }
}
